<div id="show_item"></div>

<script type="text/javascript">
  $('#sohd').keyup(function(){
      var val = $(this).val();
      $(this).val(val.substring(1, val.length));

      var val_new = $(this).val();
      if (val_new.length<7) {
          $(this).val("0"+val_new);
      }
  });

  $('.numbers').keyup(function(event) {

        // skip for arrow keys
    if(event.which >= 37 && event.which <= 40) return;

    // format number
    $(this).val(function(index, value) {
      return value
        .replace(/[^0-9-.]/g, "")
        .replace(/\B(?=(\d{3})+(?!\d))/g, ",")
      ;
    });
  });

$("#find_item").click(function(){
  var batdau = $('#batdau').val();
  var ketthuc = $('#ketthuc').val();
  var customer = $('#customer').val();
    $.ajax({
        url: '<?php echo BASE_URL ?>/vat/getItem',
        type: 'GET',
        data: {batdau:batdau, ketthuc: ketthuc, customer: customer},
        success:function(data){
          $('#show_item').html(data);
          $("#show_item" ).dialog( "open" );

          $('#tblExport2').DataTable({
              "lengthMenu": [[-1, 10, 25, 50, 100], ["Tất cả", 10, 25, 50, 100]],
              "columnDefs": [ {
                "targets": 0,
                "orderable": false
              } ],
              "fnRowCallback": function( nRow, aData, iDisplayIndex, iDisplayIndexFull ) {
                var index = iDisplayIndex +1;
                  $('.tr td:eq(0)',nRow).html(index);
                  return nRow
              }
          });

          $('.tr').click(function(){
            $(this).find('input').each(function () {
              if ($(this).is(':checked')) {
                $(this).prop('checked',false);
                if ($(this).attr('name') == 'check[]') {
                  var order = $(this).attr('data');
                  $('input[name="check_i[]"][data='+order+']').prop('checked',false);
                }
              }
              else{
                $(this).prop('checked',true);
                if ($(this).attr('name') == 'check[]') {
                  var order = $(this).attr('data');
                  $('input[name="check_i[]"][data='+order+']').prop('checked',true);
                }
                
              }
            });
          });
        }

    });
});
$( "#show_item" ).dialog({
    autoOpen: false,
    modal: true,
    width: "70%",
    maxHeight: $(window).height()-50,
    title: "Danh sách đơn hàng",
    hide: 'fold',
    show: 'blind',
    position: ['center',5],
    buttons: 
    [
      {
        text: "Chọn",
        icons: { primary: "ui-icon-check" },
        click: function(){
          var i=1;
          var j=1;
          var tong = 0;
          $('input[name="check_i[]"]:checked').each(function () {
            $('#tendv').val($(this).attr('data-cus'));
            $('#mst').val($(this).attr('data-mst'));
            $('#diachi').val($(this).attr('data-add'));

            var brand = $(this).attr('data-brand');
            var size = $(this).attr('data-size');
            var pattern = $(this).attr('data-pattern');

            
            $('.order:eq('+i+')').val($(this).attr('data'));
            $('.orderlist:eq('+i+')').val($(this).attr('value'));
            $('.brand:eq('+i+')').val(brand);
            $('.size:eq('+i+')').val(size);
            $('.pattern:eq('+i+')').val(pattern);
            $('.check:eq('+i+')').val(brand+'~'+size+'~'+pattern);

            var sl = $(this).attr('data-sl');
            var tt = $(this).attr('data-tt');
            var to = $(this).attr('data-cong');

            var check=0;
            var rowIndex = 0;
            $('.check').each(function(){
              if ($(this).val()==(brand+'~'+size+'~'+pattern) && $(this).attr('data')!=i) {
                $('.sl:eq('+rowIndex+')').val((parseFloat($('.sl:eq('+rowIndex+')').val())+parseFloat(sl)) || "");
                $('.tt:eq('+rowIndex+')').val((parseFloat($('.tt:eq('+rowIndex+')').val())+parseFloat(tt)) || "");
                $('.tt:eq('+rowIndex+')').attr('data',(parseFloat($('.tt:eq('+rowIndex+')').attr('data'))+parseFloat(to)) || "");
                $('.total_hide:eq('+rowIndex+')').val((parseFloat($('.tt:eq('+rowIndex+')').attr('data'))+parseFloat(to)) || "");
                check=1;
              }
              rowIndex++;
            });

            if (check==0) {
              $('.stt:eq('+i+')').val(j);
              $('.ten:eq('+i+')').val($(this).attr('data-ten'));
              $('.dvt:eq('+i+')').val($(this).attr('data-dvt'));
              $('.sl:eq('+i+')').val($(this).attr('data-sl'));
              $('.dg:eq('+i+')').val($(this).attr('data-dg'));
              $('.dg:eq('+i+')').attr('data',$(this).attr('data-dg1'));
              $('.tt:eq('+i+')').val($(this).attr('data-tt'));
              $('.tt:eq('+i+')').attr('data',$(this).attr('data-cong'));
              $('.price_hide:eq('+i+')').val($(this).attr('data-dg1'));
              $('.total_hide:eq('+i+')').val($(this).attr('data-cong'));

              j++;
            }
            

            tong += parseFloat($(this).attr('data-cong'));

            i++;
          });

          var k=1;
          var temp_stt="";
          var temp_ten="";
          var temp_dvt="";
          var temp_sl="";
          var temp_dg="";
          var temp_dg1="";
          var temp_tt="";
          var temp_to="";
          var temp_order="";
          var temp_orderlist="";
          var temp_brand="";
          var temp_size="";
          var temp_pattern="";
          var temp_check="";
          while(k<16){
            
            if ($('.stt:eq('+k+')').val()=="") {
              var l=k+1;
              while(l<16){
                if ($('.stt:eq('+l+')').val()!="") {
                  temp_stt = $('.stt:eq('+l+')').val();
                  temp_ten = $('.ten:eq('+l+')').val();
                  temp_dvt = $('.dvt:eq('+l+')').val();
                  temp_sl = $('.sl:eq('+l+')').val();
                  temp_dg = $('.dg:eq('+l+')').val();
                  temp_dg1 = $('.dg:eq('+l+')').attr('data');
                  temp_tt = $('.tt:eq('+l+')').val();
                  temp_to = $('.tt:eq('+l+')').attr('data');
                  temp_order = $('.order:eq('+l+')').val();
                  temp_orderlist=$('.orderlist:eq('+l+')').val();
                  temp_brand=$('.brand:eq('+l+')').val();
                  temp_size=$('.size:eq('+l+')').val();
                  temp_pattern=$('.pattern:eq('+l+')').val();
                  temp_check=$('.check:eq('+l+')').val();

                  $('.stt:eq('+l+')').val($('.stt:eq('+k+')').val());
                  $('.ten:eq('+l+')').val($('.ten:eq('+k+')').val());
                  $('.dvt:eq('+l+')').val($('.dvt:eq('+k+')').val());
                  $('.sl:eq('+l+')').val($('.sl:eq('+k+')').val());
                  $('.dg:eq('+l+')').val($('.dg:eq('+k+')').val());
                  $('.dg:eq('+l+')').attr('data',$('.dg:eq('+k+')').attr('data'));
                  $('.tt:eq('+l+')').val($('.tt:eq('+k+')').val());
                  $('.tt:eq('+l+')').attr('data',$('.tt:eq('+k+')').attr('data'));
                  $('.order:eq('+l+')').val($('.order:eq('+k+')').val());
                  $('.orderlist:eq('+l+')').val($('.orderlist:eq('+k+')').val());
                  $('.brand:eq('+l+')').val($('.brand:eq('+k+')').val());
                  $('.size:eq('+l+')').val($('.size:eq('+k+')').val());
                  $('.pattern:eq('+l+')').val($('.pattern:eq('+k+')').val());
                  $('.check:eq('+l+')').val($('.check:eq('+k+')').val());

                  $('.price_hide:eq('+l+')').val($('.dg:eq('+k+')').attr('data'));
                  $('.total_hide:eq('+l+')').val($('.tt:eq('+k+')').attr('data'));

                  $('.stt:eq('+k+')').val(temp_stt);
                  $('.ten:eq('+k+')').val(temp_ten);
                  $('.dvt:eq('+k+')').val(temp_dvt);
                  $('.sl:eq('+k+')').val(temp_sl);
                  $('.dg:eq('+k+')').val(temp_dg);
                  $('.dg:eq('+k+')').attr('data',temp_dg1);
                  $('.tt:eq('+k+')').val(temp_tt);
                  $('.tt:eq('+k+')').attr('data',temp_to);
                  $('.order:eq('+k+')').val(temp_order);
                  $('.orderlist:eq('+k+')').val(temp_orderlist);
                  $('.brand:eq('+k+')').val(temp_brand);
                  $('.size:eq('+k+')').val(temp_size);
                  $('.pattern:eq('+k+')').val(temp_pattern);
                  $('.check:eq('+k+')').val(temp_check);

                  $('.price_hide:eq('+k+')').val(temp_dg1);
                  $('.total_hide:eq('+k+')').val(temp_to);

                  break;
                }
                l++;
              }
            }
            k++;
          }
          
          

          var tienthue = parseFloat(Math.round(tong*0.1));
          var tongcong = Math.round(tong+tienthue);
          var congtien = Math.round(tong);

          $('#tienthue').val(tienthue);
          $('#tongcong').val(tongcong);
          $('#congtien').val(congtien);

          $.ajax({
            url: '<?php echo BASE_URL ?>/vat/getMoney',
            type: 'GET',
            data: {data:tongcong},
            success:function(data){
              $('#tongcongchu').val(data.trim());
            },
          });

          $('.numbers').val(function(index, value) {
            return value
              .replace(/[^0-9-.]/g, "")
              .replace(/\B(?=(\d{3})+(?!\d))/g, ",")
            ;
          });


          $('.sl').keyup(function(){
              var rowIndex=$(this).closest('tr').index()-12;
              
              var sl = parseInt($(this).val()) || 0;
              var dg = parseFloat($('.dg:eq('+rowIndex+')').attr('data').replace(/\,/g,'')) || 0;
              var tt = sl*dg;

              if (tt>0) {
                $('.tt:eq('+rowIndex+')').val(Math.round(tt));
                $('.tt:eq('+rowIndex+')').attr('data',tt);

                $('.total_hide:eq('+rowIndex+')').val(tt);

                var tong = 0;
                $('.tt').each(function(){
                  if ($(this).val() != "") {
                    tong += parseFloat($(this).attr('data') || 0);
                  }
                  
                });

                var tienthue = parseFloat(Math.round(tong*0.1));
                var tongcong = Math.round(tong+tienthue);
                var congtien = Math.round(tong);

                $('#tienthue').val(tienthue);
                $('#tongcong').val(tongcong);
                $('#congtien').val(congtien);

                $.ajax({
                  url: '<?php echo BASE_URL ?>/vat/getMoney',
                  type: 'GET',
                  data: {data:tongcong},
                  success:function(data){
                    $('#tongcongchu').val(data.trim());
                  },
                });

                $('.numbers').val(function(index, value) {
                  return value
                    .replace(/[^0-9-.]/g, "")
                    .replace(/\B(?=(\d{3})+(?!\d))/g, ",")
                  ;
                });
              }

            });

            $('.dg').keyup(function(){
              var rowIndex=$(this).closest('tr').index()-12;
              
              var dg = parseFloat($(this).val().replace(/\,/g,'')) || 0;
              var sl = parseInt($('.sl:eq('+rowIndex+')').val()) || 0;
              var tt = sl*dg;

              $(this).attr('data',dg);
              $('.price_hide:eq('+rowIndex+')').val(dg);

              if (tt>0) {
                $('.tt:eq('+rowIndex+')').val(Math.round(tt));
                $('.tt:eq('+rowIndex+')').attr('data',tt);

                $('.total_hide:eq('+rowIndex+')').val(tt);

                var tong = 0;
                $('.tt').each(function(){
                  if ($(this).val() != "") {
                    tong += parseFloat($(this).attr('data') || 0);
                  }
                  
                });

                var tienthue = parseFloat(Math.round(tong*0.1));
                var tongcong = Math.round(tong+tienthue);
                var congtien = Math.round(tong);

                $('#tienthue').val(tienthue);
                $('#tongcong').val(tongcong);
                $('#congtien').val(congtien);

                $.ajax({
                  url: '<?php echo BASE_URL ?>/vat/getMoney',
                  type: 'GET',
                  data: {data:tongcong},
                  success:function(data){
                    $('#tongcongchu').val(data.trim());
                  },
                });

                $('.numbers').val(function(index, value) {
                  return value
                    .replace(/[^0-9-.]/g, "")
                    .replace(/\B(?=(\d{3})+(?!\d))/g, ",")
                  ;
                });
              }

            });


          $( "#show_item" ).dialog( "close" );
        }
      },
      {
        text: "Đóng",
        icons: { primary: "ui-icon-close" },
        click: function(){
          $( "#show_item" ).dialog( "close" );
        }
      },

    ]
});

