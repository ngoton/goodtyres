$('#print').click(function(){
  var ngay = $('#ngay').val().trim();
  var thang = $('#thang').val().trim();
  var nam = $('#nam').val().trim();
  var sohd = $('#sohd').val().trim();

  if (ngay=="" || thang=="" || nam=="") {
    alert('Vui lòng nhập vào ngày tháng!');
    return;
  }
  else if (sohd=="" || sohd=="0000000" || sohd==0) {
    alert('Vui lòng nhập vào số hóa đơn!');
    return;
  }
  else{
    var r = confirm("Bạn có chắc chắn không?");
    if (r == true){
      $('#printhd').submit();
    }
  }

});

