    function countDays($year, $month, $ignore) {
        $count = 0;
        $counter = mktime(0, 0, 0, $month, 1, $year);
        while (date("n", $counter) == $month) {
            $day = date("w", $counter);
            if (in_array($day, $ignore) == false) {
                if ($day==6) {
                    $count += 0.5;
                }
                else{
                    $count++;
                }
                
            }
            $counter = strtotime("+1 day", $counter);
        }
        return $count;
    }

